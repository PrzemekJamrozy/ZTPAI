                    "gender": {
                        "description": "Either MALE or FEMALE",
                        "type": "string",
                        "example": "MALE"
                    }
                },
                "type": "object"
            },
            "UserOnboardingInput": {
                "description": "Additional data of user",
                "properties": {
                    "bio": {
                        "type": "string",
                        "example": "Example bio"
                    },
                    "fbLink": {
                        "type": "string",
                        "example": "https://facebook.com"
                    },
                    "igLink": {
                        "type": "string",
                        "example": "https://instagram.com"
                    },
                    "preferredGender": {
                        "description": "Either MALE or FEMALE",
                        "type": "string",
                        "example": "MALE"
                    },
                    "avatar": {
                        "description": "Base64 encoded image",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "UserUpdateDto": {
                "description": "User update",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Jan"
                    },
                    "surname": {
                        "type": "string",
                        "example": "Kowalski"
                    },
                    "email": {
                        "type": "string",
                        "example": "jan@example.com"
                    },
                    "password": {
                        "type": "string",
                        "example": "password"
                    },
                    "gender": {
                        "description": "Either MALE or FEMALE",
                        "type": "string",
                        "example": "MALE"
                    }
                },
                "type": "object"
            },
            "DeletedModelResource": {
                "description": "Delete user",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    }
                },
                "type": "object"
            },
            "UserResource": {
                "description": "User",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Jan Kowalski"
                    },
                    "surname": {
                        "type": "string",
                        "example": "jan@example.com"
                    },
                    "email": {
                        "type": "string",
                        "example": "jan@example.com"
                    },
                    "status": {
                        "description": "Either ACTIVE or DURING_REGISTER",
                        "type": "string",
                        "example": "ACTIVE"
                    },
                    "gender": {
                        "description": "Either MALE or FEMALE",
                        "type": "string",
                        "example": "MALE"
                    },
                    "roles": {
                        "description": "Array of roles of user in system",
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "USER",
                                "ADMIN"
                            ]
                        },
                        "example": [
                            "USER"
                        ]
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
