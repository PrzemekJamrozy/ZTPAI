    "components": {
        "schemas": {
            "UserAdminUpdateDto": {
                "description": "User update",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Jan"
                    },
                    "surname": {
                        "type": "string",
                        "example": "Kowalski"
                    },
                    "email": {
                        "type": "string",
                        "example": "jan@example.com"
                    },
                    "password": {
                        "type": "string",
                        "example": "password"
